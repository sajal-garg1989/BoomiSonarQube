          <Configuration>
            <Scripting language="groovy" preserveOrder="true"
            useComponent="false">
              <ScriptToExecute>//Build the labor category portion
              of the transfer string based on how many labor
              categories are configured if (LC1=="" &amp;&amp;
              LC2=="" &amp;&amp; LC3=="" &amp;&amp; LC4==""
              &amp;&amp; LC5=="" &amp;&amp; LC6=="") {output="";}
              else if (laborcat_count== "0") {output="";} else if
              (laborcat_count== "1") {output=LC1;} else if
              (laborcat_count== "2") {output=LC1 + "," + LC2;} else
              if (laborcat_count== "3") {output=LC1 + "," + LC2 +
              "," + LC3;} else if (laborcat_count== "4")
              {output=LC1 + "," + LC2 + "," + LC3 + "," + LC4;}
              else if (laborcat_count== "5") {output=LC1 + "," +
              LC2 + "," + LC3 + "," + LC4 + "," + LC5;} else if
              (laborcat_count== "6") {output=LC1 + "," + LC2 + ","
              + LC3 + "," + LC4 + "," + LC5 + "," +
              LC6;}</ScriptToExecute>
              <Input dataType="character" index="1"
              name="laborcat_count" />
